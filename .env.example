# ArmorIQ Environment Configuration
# ===================================
# Copy this file to .env and fill in values

# ArmorIQ API Key (for Intent Authentication Protocol)
# Get from: https://platform.armoriq.ai/dashboard/api-keys
# Must start with ak_live_ or ak_test_
ARMORIQ_API_KEY=

# Optional: ArmorIQ Endpoints (defaults shown)
ARMORIQ_IAP_ENDPOINT=https://iap.armoriq.ai
ARMORIQ_PROXY_ENDPOINT=https://proxy.armoriq.ai
ARMORIQ_USER_ID=hr-swarm-demo
ARMORIQ_AGENT_ID=hr-agent

# Gemini API Key (for LLM-powered agent)
# Get from: https://aistudio.google.com/apikey
# Leave empty for mock mode (works without AI)
GEMINI_API_KEY=AIzaSyClODiL6scoJ9VZHk6YFhyA07ccGyPhj0E

# Optional: Slack Integration
# Webhook URL for sending alerts to Slack
SLACK_WEBHOOK_URL=

# Optional: Microsoft Teams Integration
# Webhook URL for sending alerts to Teams
TEAMS_WEBHOOK_URL=

# Optional: Alert Channels (comma-separated)
ALERT_CHANNELS=

# Optional: TIRS Audit Signing Key
# Custom key for cryptographic audit signatures
# Default: tirs-demo-key-2026
TIRS_SIGNING_KEY=
